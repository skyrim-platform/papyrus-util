{
    "name": "@skyrim-platform/papyrus-util",
    "version": "4.2.3",
    "description": "TypeScript library for the PapyrusUtil Skyrim modding utility",
    "license": "MIT",
    "homepage": "https://github.com/skyrim-platform/papyrus-util",
    "repository": {
        "type": "git",
        "url": "https://github.com/skyrim-platform/papyrus-util.git"
    },
    "scripts": {
        "build": "tsc",
        "watch": "tsc --watch",
        "publish": "npm run build && cpy \"dist/*\" . && npm publish && npm run cleanup",
        "cleanup": "rimraf \"*.js\" \"*.ts\" dist/"
    },
    "devDependencies": {
        "cpy": "^8.1.2",
        "cpy-cli": "^3.1.1",
        "rimraf": "^3.0.2",
        "tsc": "^2.0.3",
        "typescript": "^4.5.4"
    },
    "files": [
        "*.js",
        "*.ts"
    ],
    "keywords": [
        "skyrim",
        "skyrim platform"
    ],
    "dependencies": {
        "@skyrim-platform/skyrim-platform": "^2.3.0"
    }
}
